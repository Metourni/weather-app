    "lint-staged": "^9.4.2",
    "node-sass": "^7.0.0",
    "prettier": "^1.18.2",
    "react-app-rewired": "^2.1.4",
    "stylelint": "^11.1.1",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0"
  },
  "engines": {
    "node": "12.14.1",
    "npm": "6.13.4"
  },
  "eslintConfig": {
